generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id          String   @id @default(cuid())
    title       String
    name        String
    surname     String
    email       String   @unique
    ethnicity   String
    gender      String
    phoneNumber String
    idNumber    String
    dateOfBirth String
    password    String
    createdAt   DateTime @default(now())
    updatedAt   DateTime @updatedAt
}
